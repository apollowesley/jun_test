<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<settings>
		<setting name="lazyLoadingEnabled" value="true" />
		<setting name="aggressiveLazyLoading" value="false" />
	</settings>
	<typeAliases>
		<typeAlias type="com.zb.common.vo.UserVo" alias="UserVo" />
		<typeAlias type="com.zb.common.vo.RoleVo" alias="RoleVo" />
		<typeAlias type="com.zb.common.vo.PermissionVo" alias="PermissionVo" />
	</typeAliases>
	<typeHandlers>
		<typeHandler handler="com.zb.common.enums.entity.EntityEnumTypeHandler" javaType="com.zb.common.enums.Status" />
		<typeHandler handler="com.zb.common.enums.entity.EntityEnumTypeHandler" javaType="com.zb.common.enums.UserStatus" />
	</typeHandlers>

</configuration>