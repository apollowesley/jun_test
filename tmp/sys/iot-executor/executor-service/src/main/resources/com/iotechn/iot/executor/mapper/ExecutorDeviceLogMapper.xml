<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.iotechn.iot.executor.mapper.ExecutorDeviceLogMapper">

    <select id="selectKeys" resultType="java.lang.String" parameterType="java.lang.String">
        SELECT DISTINCT
            log_key
        FROM
            executor_device_log_do
        WHERE
            secret_key = #{secretKey}
    </select>
</mapper>