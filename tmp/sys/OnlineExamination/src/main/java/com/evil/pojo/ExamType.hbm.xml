<?xml version="1.0" ?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.evil.pojo">
	<class name="ExamType" table="tb_examType">
		<id name="id" type="string">
			<column name="ID" length="32" />
			<generator class="uuid.hex"></generator>
		</id>
		<property name="name" column="name" type="string" length="50" />
		<property name="typeDescribe" column="typeDescribe" type="string" length="2147483647"  />
		<property name="addTime" column="endaddTimeTime" type="timestamp" update="false" />
		
	</class>
</hibernate-mapping>