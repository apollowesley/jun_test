<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="system.admin.role">

    <select id="system.admin.role.findByUserId" resultType="map">
        select * from system_admin_role where admin_id = #{adminId}
    </select>

    <insert id="system.admin.role.batchSave" parameterType="map">
        insert into system_admin_role(admin_id, role_id)
        values
        <foreach collection="list" separator="," item="item">
            (#{item.adminId}, #{item.roleId})
        </foreach>
    </insert>
</mapper>