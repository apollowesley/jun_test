.search-txt{
  color: #707070;
  line-height: 70rpx;
  height: 70rpx;
  text-align: center;

}
.search-txt input{
  width: 100%;
  line-height: 70rpx;
  height: 70rpx;
}

.search{
  background-color: rgb(161, 179, 230);
  color: white;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  width: 13%;
  position: fixed;
  right: 20rpx;
  margin-top: 25rpx;
  border-radius: 10rpx;
}

.orderList {
    padding: 0 26rpx 0;
    margin-top: 115rpx;
}

.orderList .items {
    background-color: #fff;
    margin-top: 24rpx;
    color: #7a7a7a;
    border-radius: 12rpx;
}

.orderList .hd {
    height: 76rpx;
    line-height: 74rpx;
    border-bottom: 2rpx #eee dotted;
    padding:0 20rpx 0 100rpx;
    font-size: 32rpx;
    position: relative;
}

.orderList .hd .r {
    float: right;
    font-size: 28rpx
}

.orderList .hd .l {
    white-space:nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    word-break: break-all;
    word-wrap: break-word;
}

.orderList .hd::before {
    width: 60rpx;
    height: 74rpx;
    line-height: 74rpx;
    font-family: iconfont;
    content: "\e620";
    position: absolute;
    left: 26rpx;
    top: 0;
    font-size: 48rpx;
    color: #7aa6d1;
}

.orderList .bd {
    padding: 20rpx 0 30rpx 34rpx;
}

.orderList .t {
    display: -webkit-box;
    height: 50rpx;
    line-height: 50rpx;
    overflow: hidden;
    padding-right: 34rpx
}

.orderList .t .l, .orderList .t .c, .orderList .t .r {
    padding-right: 20rpx;
    white-space:nowrap;
    overflow: hidden;
    display: block;
    text-overflow: ellipsis;
    word-break: break-all;
    word-wrap: break-word;
}

.orderList .t .l {
    width: 50%;
}

.orderList .t .c {
    width: 50%;
    text-align: right;
}

.orderList .t .r {
    width: 240rpx;
    text-align: right;
    padding-right: 0;
}

.orderList .middle {
    height: 50rpx;
    line-height: 50rpx;
    width: 100%;
    overflow: hidden;
}

.orderList .middle .l,.orderList .middle .c {
    width:50%;
    float: left;
    white-space:nowrap;
    overflow: hidden;
    display: block;
    text-overflow: ellipsis;
    word-break: break-all;
    word-wrap: break-word;
    box-sizing: border-box;
    padding-right:34rpx;
}
.orderList .middle .c {
    text-align: right;
}
