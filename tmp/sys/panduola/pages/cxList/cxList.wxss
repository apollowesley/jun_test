.search-item{
  width: 100%;
  height: 120rpx;
  background: #e3e3e3;
  position: fixed;
  top: 0;
  display: flex;
  z-index: 999;
}
.search-item1{
  width: 33%;
  height: 70rpx;
  line-height: 70rpx;
  background: #fff;
  border-radius: 10rpx;
  margin-left: 20rpx;
