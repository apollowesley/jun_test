<view class="banner"></view>

<view class='top-tab'>
    <view class='tab-item' data-proNo="10" data-proName="进件" bindtap='goIncomingPorts'>
        <view class='tab-item-l'>
            <view class='l-top'>开点</view>
            <view class='l-bt'>Open The Point</view>
        </view>
        <view class='tab-item-r'><image src='../../images/icon/icon-home-01.png'></image></view>
    </view>
    <view class='tab-item' bindtap='goPosXj'>
        <view class='tab-item-l'>
            <view class='l-top'>点名</view>
            <view class='l-bt'>Roll Call Form</view>
        </view>
        <view class='tab-item-r'><image src='../../images/icon/icon-home-02.png'></image></view>
    </view>
    <view class='tab-item' bindtap='goPosInstall'>
        <view class='tab-item-l'>
            <view class='l-top'>统计</view>
            <view class='l-bt'>Statistical Form</view>
        </view>
        <view class='tab-item-r'><image src='../../images/icon/icon-home-03.png'></image></view>
    </view>
    <view class='tab-item' bindtap='goPosSearch'>
        <view class='tab-item-l'>
            <view class='l-top'>查询</view>
            <view class='l-bt'>Query Search</view>
        </view>
        <view class='tab-item-r'><image src='../../images/icon/icon-home-24.png'></image></view>
    </view>
</view>

