.banner{
    height: 200rpx;
    line-height: 200rpx;
    background: #e7f3ff;
    color: #3e92f4;
    font-size: 45rpx;
    text-align: center;
    margin-bottom: 20rpx;
}

.section{
    height: 100rpx;
    line-height: 100rpx;
    background: #FFF;
    margin-bottom: 20rpx;
    padding: 0 20rpx;
}
.btn-box{
    padding: 40rpx 20rpx;
}
.btn-box .btn{
    width: 100%;
    height: 88rpx;
    line-height: 88rpx;
    text-align: center;
    color: #fff;
    background: #3e92f4;
    border-radius: 35rpx;
    font-size: 32rpx;
}