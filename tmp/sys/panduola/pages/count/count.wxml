<view class='banner'>排名查询</view>

<view class='wrap'>

    <view class="section">
        <picker
            mode="date"
            value="{{date}}"
            start="2015-09-01"
            end="2099-09-01"
            bindchange="bindStartDateChange"
        >
            <view class="picker">
            开始时间: {{beginDate}}
            </view>
        </picker>
    </view>

    <view class="section">
        <picker
            mode="date"
            value="{{date}}"
            start="2015-09-01"
            end="2099-09-01"
            bindchange="bindEndDateChange"
        >
            <view class="picker">
            结束时间: {{endDate}}
            </view>
        </picker>
    </view>

    <view class='btn-box'>
        <view class='btn' bindtap='viewCountList' data-beginDate="{{beginDate}}" data-endDate="{{endDate}}">查询排名</view>
    </view>

</view>