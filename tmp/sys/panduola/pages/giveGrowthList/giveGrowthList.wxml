<view class='banner'>统计结果</view>

<view class='wrap'>
    <view class='top'>
        <view class='top-l'>获得者</view>
        <view class='top-c'>点数</view>
        <view class='top-r'>说明/时间</view>
    </view>
    <view class='item' wx:for="{{list}}" wx:key="id">
        <view class='item-l'>{{item.gainer}}</view>
        <view class='item-c'>{{item.growthValue}}</view>
        <view class='item-r'>{{item.description}} / {{item.timeDate}}</view>
    </view>

</view>