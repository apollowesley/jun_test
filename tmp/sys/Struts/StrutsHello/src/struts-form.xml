<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

    <package name="form" extends="struts-default" namespace="/form">

        <action name="toFormLogin" class="com.gs.struts.FormTagAction" method="toLogin">
            <result>/WEB-INF/pages/form-tags.jsp</result>
        </action>

        <action name="formLogin" class="com.gs.struts.FormTagAction" method="login">
            <result>/WEB-INF/pages/form-tags-info.jsp</result>
        </action>

    </package>
</struts>