<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="kuaiDaiLiParseActor" class="com.lookup.dynamic.ext.ActorFactoryBean">
        <!-- actor system reference -->
        <property name="actorSystem" ref="actorSystem"/>
        <!-- class name of actor to create -->
        <property name="actorClass" value="com.lookup.dynamic.parser.imp.KuaiDaiLiParser"/>

    </bean>

    <bean id="wikiIataListParser" class="com.lookup.dynamic.ext.ActorFactoryBean">
        <!-- actor system reference -->
        <property name="actorSystem" ref="actorSystem"/>
        <!-- class name of actor to create -->
        <property name="actorClass" value="com.lookup.dynamic.parser.imp.WikiIataListParser"/>

    </bean>
</beans>