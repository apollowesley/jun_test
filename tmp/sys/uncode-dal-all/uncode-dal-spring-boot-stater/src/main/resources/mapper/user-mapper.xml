<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.uncode.dal.springboot.mapper.UserMapper">
	<select id="users" resultType="cn.uncode.dal.springboot.dto.User">
		SELECT
			*
		FROM
			user where id = 1;
	</select>
	
	<insert id="create" parameterType="cn.uncode.dal.springboot.dto.User" useGeneratedKeys="true" keyColumn="id" keyProperty="id">
		INSERT INTO user(id,age,birthDay,passWord,userName) VALUES (#{id},#{age},#{birthDay},#{passWord},#{userName});
	</insert>
</mapper>