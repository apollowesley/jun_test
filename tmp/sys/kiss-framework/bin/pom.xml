	<build>
		<resources>
			<resource>
				<filtering>false</filtering>
				<directory>src/main/config</directory>
				<targetPath>${project.build.outputDirectory}/config</targetPath>
				<includes>
					<include>**</include>
				</includes>
			</resource>
			<resource>
				<filtering>false</filtering>
				<directory>src/main/config</directory>
				<targetPath>${project.basedir}/target/test-classes/config</targetPath>
				<includes>
					<include>**</include>
				</includes>
			</resource>
			<resource>
				<filtering>false</filtering>
				<directory>src/main/resources</directory>
				<targetPath>${project.build.outputDirectory}/resources</targetPath>
				<excludes>
					<exclude>**/jrebel.xml</exclude>
				</excludes>
			</resource>
			<resource>
				<filtering>false</filtering>
				<directory>src/main/resources</directory>
				<targetPath>${basedir}/target/test-classes/resources</targetPath>
				<excludes>
					<exclude>**/rebel.xml</exclude>
				</excludes>
			</resource>
			<resource>
				<filtering>false</filtering>
				<directory>src/main/java</directory>
				<includes>
					<include>**/*.xml</include>
					<include>**/*.properties</include>
					<include>**/*.bsh</include>
					<include>**/*.vm</include>
				</includes>
				<excludes>
					<exclude>**/rebel.xml</exclude>
				</excludes>
			</resource>
		</resources>
