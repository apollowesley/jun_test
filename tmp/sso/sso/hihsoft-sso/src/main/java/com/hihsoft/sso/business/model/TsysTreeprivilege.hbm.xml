<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.hihsoft.sso.business.model">
    <class name="TsysTreeprivilege" table="T_SYS_TREEPRIVILEGE">

        <id name="treeid" column="TREEID" type="java.lang.String" length="32">
            <generator class="uuid"/>
        </id>
		
		
		
		<property 
			name="orgid" 
			column="ORGID" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="32"/>
		
		<property 
			name="userid" 
			column="USERID" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="32"/>
		
		<!-- One-To-Many attributes -->

		<!-- Many-To-One attributes -->

    </class>
</hibernate-mapping>

