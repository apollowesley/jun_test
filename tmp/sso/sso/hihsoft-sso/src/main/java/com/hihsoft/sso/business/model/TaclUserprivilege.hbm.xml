<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.hihsoft.sso.business.model">
    <class name="TaclUserprivilege" table="T_ACL_USERPRIVILEGE">

        <id name="userprviid" column="USERPRVIID" type="java.lang.String" length="32">
            <generator class="uuid"/>
        </id>
		
		
		
		<property 
			name="moduleid" 
			column="MODULEID" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="32"/>
		
		<property 
			name="operateid" 
			column="OPERATEID" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="32"/>
		
		<property 
			name="userid" 
			column="USERID" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="32"/>
		
    </class>
</hibernate-mapping>

