<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.hihsoft.sso.sysmonitor.syslogs.model">
	 <class name="TlogServicecall" table="T_LOG_SERVICECALL">

        <id name="callid" column="CALLID" type="java.lang.String" length="32">
            <generator class="uuid"/>
        </id>
			<property 
			name="serviceClass" 
			column="SERVICE_CLASS" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="200"/>
		
		<property 
			name="userid" 
			column="USERID" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="32"/>
		
		<property 
			name="methodName" 
			column="METHOD_NAME" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="100"/>
		
		<property 
			name="consumeTime" 
			column="CONSUME_TIME" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="30"/>
		
		<property 
			name="callTime" 
			column="Call_TIME" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="20"/>
		
		<!-- One-To-Many attributes -->

		<!-- Many-To-One attributes -->

    </class>
</hibernate-mapping>

