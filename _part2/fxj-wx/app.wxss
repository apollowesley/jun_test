}

@font-face {
  font-family: 'iconfont';  /* project id 1029094 */
  src: url('//at.alicdn.com/t/font_1029094_k2uw79ukea.eot');
  src: url('//at.alicdn.com/t/font_1029094_k2uw79ukea.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_1029094_k2uw79ukea.woff2') format('woff2'),
  url('//at.alicdn.com/t/font_1029094_k2uw79ukea.woff') format('woff'),
  url('//at.alicdn.com/t/font_1029094_k2uw79ukea.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_1029094_k2uw79ukea.svg#iconfont') format('svg');
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 44rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.borderbox {
    margin-bottom: 20rpx;
    background-color: #fff;
    overflow: hidden;
    border: none;
}

.borderbox:after {
    content: "\200B";
    display: block;
    clear: both;
    height: 0;
}

.borderbox.mb0 {
    margin-bottom: 0;
}

.tbviewlist {
    background: #fff;
    overflow: hidden;
}

.tbviewlist .items {
    line-height: 30px;
    padding:12px 36px 12px 15px;
    border-bottom: 1px solid #eee;
    position: relative;
    width: 100%;
    box-sizing: border-box;
}

.tbviewlist .items::after {
    position: absolute;
    content: "\e60a";
    color: #ccc;
    font-family: iconfont;
    width: 28px;
    height: 20px;
    line-height: 20px;
    text-align: center;
    right: 6px;
    top: 50%;
    margin-top: -10px;
    font-size:22px;
}

.tbviewlist .items.onafter {
    padding-right: 15px;
}

.tbviewlist .items.onafter::after {
    content: "";
}

.tbviewlist .items .r {
    float: right;
    color: #888;
}

.tbviewlist .items .r .em {
    height: 20px;
    line-height: 20px;
    background-color: #64dce6;
    color: #fff;
    display: inline-block;
    padding: 0 8px;
    -webkit-border-radius: 10px;
    border-radius: 10px;
}

.tbviewlist .items .c {
    white-space: nowrap;
    overflow: hidden;
    display: block;
    -ms-text-overflow: ellipsis;
    text-overflow: ellipsis;
}

.tbviewlist .items .c .span {
    padding-left: 36rpx;
    color: #64dce6;
}

.tbviewlist .items.heightauto {
    line-height: 60px;
}

.tbviewlist .items.heightauto .userhead {
    width: 60px;
    height: 60px;
    border-radius: 50%;
    background-repeat: no-repeat;
    background-position: center;
    background-size: cover;
    overflow: hidden;
}
.tbviewlist .items.heightauto .userhead image {
    width: 60px;
    height: 60px;
    border-radius: 50%;
    overflow: hidden;
}
.tbviewlist .items .ico {
    width: 60rpx;
    height:60rpx;
    display: inline-block;
    vertical-align: middle;
    text-align: center;
}
.tbviewlist .items .ico::after {
    color: #7aa6d1;
    font-family: iconfont;
}

.ico-adopt::after {
    content: "\e722";
    font-size:40rpx;
}

.ico-doubt::after {
    content: "\e676";
    font-size:40rpx;
}

.placeholder {
    color: #999;
}

.centertabbar {
    width: 100%;
    height: 100rpx;
    display: -webkit-box;
    display: -moz-box;
    display: -ms-flexbox;
    display: box;
    background: #fff;
    position: fixed;
    z-index:100;
    top: 0;
    left: 0;
}

.centertabbar .items {
    -webkit-box-flex: 1;
    -moz-box-flex: 1;
    -ms-box-flex: 1;
    box-flex: 1;
    width: 100%;
    height: 100rpx;
    text-align: center;
    line-height: 100rpx;
}

.centertabbar .items text {
    display: inline-block;
    padding:0 20rpx;
}

.centertabbar .items.active text {
    border-bottom: 4rpx solid #7aa6d1;
    height: 98rpx;
    color: #7aa6d1;
}

.viewlist {
    background: #fff;
}

.viewlist .items {
    padding: 20rpx 36rpx;
    border-bottom: 2rpx solid #eee;
    display: -webkit-box;
    line-height: 60rpx;
}
.viewlist .items.block {display: block;}
.viewlist .items.noborder {
    border-bottom: none;
}

.viewlist .hd {
    min-width: 140rpx;
    padding-right: 20rpx;
}

.viewlist .bd {
    -webkit-box-flex: 1;
    -moz-box-flex: 1;
}

.viewlist .bd.relative {
    position: relative;
}

.viewlist .bd.relative::after {
    position: absolute;
    top:0;
    right:0;
    width: 70rpx;
    text-align: center;
    height: 60rpx;
    line-height: 60rpx;
    font-family: iconfont;
    content: "\e7cc";
    font-size: 44rpx;
    color: #999999;
    z-index: 0;
}

.viewlist .bd input {
    height: 60rpx;
}

.viewlist .bd input.pr {
    padding-right: 80rpx;
    position: relative;
    z-index: 2;
}


.annexMain {
    width: 100%;
    overflow: hidden;
}

.annexMain .items {
    width: 100%;
    padding:20rpx 84rpx;
    border-bottom: 1px solid #eeeeee;
    position: relative;
    box-sizing: border-box;
}

.annexMain .items::before {
    content: '';
    width:22rpx;
    height:22rpx;
    position: absolute;
    border-radius: 50%;
    background-color: #248ffb;
    left:20rpx;
    top:41rpx;
}

.annexMain .items::after {
    width:44rpx;
    height: 46rpx;
    line-height: 46rpx;
    text-align: center;
    position: absolute;
    right:20rpx;
    top:50%;
    margin-top: -23rpx;
    font-family: iconfont;
    content: "\e60a";
    font-size: 44rpx;
    color: #999999;
}

.annexMain .title {
    font-size: 36rpx;
    line-height: 64rpx;
}

.annexMain .p {
    color: #999999;
}

.videupload {
    width:587rpx;
    height: 400rpx;
    margin: 20rpx auto 40rpx;
    border: 2rpx dashed #dfdfdf;
    background-position: center;
    background-repeat: no-repeat;
    position: relative;
    overflow: hidden;
    background-size: 144rpx auto;
    background-image: url(data:image/png;base64,);
}

.videupload video {
    width: 100%;
    height: 100%;
}

.videotape {
    text-align: center;
    padding-bottom: 40rpx;
}

.videotape .upload, .videotape .delete {
    width: 96rpx;
    height: 96rpx;
    line-height: 92rpx;
    text-align: center;
    -webkit-border-radius: 50%;
    border-radius: 50%;
    display: inline-block;
    margin: 0 40rpx;
}

.videotape .upload {
    border: 2rpx solid #248ffb;
    color: #248ffb;
}

.videotape .delete {
    border: 2rpx solid #ccc;
    color: #7a7a7a;
}

.noData {
    text-align: center;
}

.noData image {
    width: 240rpx;
    height: 240rpx;
    display: block;
    margin: auto;
}

.noData .text {
    line-height: 30px;
    color: #999;
}


.tips-upwarp {
    padding: 10px 0;
    text-align: center;
}

.tips-upwarp .upwarp-tip {
    display: inline-block;
    font-size: 12px;
    color: gray;
    vertical-align: middle;
}

.tips-upwarp .upwarp-progress {
    display: inline-block;
    width: 16px;
    height: 16px;
    border-radius: 50%;
    border: 1px solid gray;
    margin-right: 8px;
    border-bottom-color: transparent;
    vertical-align: middle;
}

.tips-rotate {
    -webkit-animation: tipsRotate 0.6s linear infinite;
    animation: tipsRotate 0.6s linear infinite;
}

@keyframes tipsRotate {
    0% {
        transform: rotate(0deg);
    }

    100% {
        transform: rotate(360deg);
    }
}