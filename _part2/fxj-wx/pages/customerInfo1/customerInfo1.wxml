<view class='ocr mt'>
    <view class="title">个人资料</view>
    <view class='data'>
        <view class='items'>
            <view class='hd'>是否有子女:</view>
            <view class='bd'>{{orderBy4}}</view>
        </view>
        <view class='items'>
            <view class='hd'>学历:</view>
            <view class='bd'>{{orderBy5}}</view>
        </view>
        <view class='items'>
            <view class='hd'>工作单位:</view>
            <view class='bd'>{{orderBy6}}</view>
        </view>
        <view class='items'>
            <view class='hd'>单位地址:</view>
            <view class='bd'>{{orderBy7}}</view>
        </view>
        <view class='items'>
            <view class='hd'>月收入:</view>
            <view class='bd'>{{orderBy8}}</view>
        </view>
        <view class='items'>
            <view class='hd'>储蓄卡:</view>
            <view class='bd'>{{orderBy9}}</view>
        </view>

        <view class='items'>
            <view class='hd'>居住地址:</view>
            <view class='bd'>{{residentialAddress}}</view>
        </view>
        <view class='items'>
            <view class='hd'>联系电话:</view>
            <view class='bd'>{{telephone}}</view>
        </view>
        <view class='items'>
            <view class='hd'>婚姻状况:</view>
            <view class='bd'>{{checkedValue}}</view>
        </view>
        <view wx:for="{{orderEmerList}}" wx:key="id" wx:if="{{orderEmerList.length > 0}}">
            <view class='items'>
                <view class='hd'>紧急联系人:</view>
                <view class='bd'>{{item.emergencyContact}}</view>
            </view>
            <view class='items'>
                <view class='hd'>联系电话:</view>
                <view class='bd'>{{item.emergencyMobile}}</view>
            </view>
        </view>
    </view>
</view>

