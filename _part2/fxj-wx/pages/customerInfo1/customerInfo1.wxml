<view class='ocr mt'>
    <view class="title">上传附件</view>
    <view class='imgMain'>
        <view class='items' wx:for="{{fxjProAttList}}" wx:key="id" wx:for-index="listIdx">
            <view class='hd'>
                <view class='con'>
                    <view class='ico'></view>
                    <view class='text'>{{item.attName}}</view>
                </view>
            </view>
            <view class='bd'>
                <view class='uploadimg'>
                    <view class='items' wx:for="{{item.fxjOrderAttaList}}" wx:key="id">
                        <view class='pic' style='background-image:url({{item.attaUrl}})' data-list-idx="{{listIdx}}" data-img-idx="{{index}}" bindtap="previewImage"></view>
                    </view>
                </view>
            </view>
        </view>
    </view>
</view>