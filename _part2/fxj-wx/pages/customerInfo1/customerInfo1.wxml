<view wx:if="{{proNo == 30 || proNo == 40}}">
    <view class='ocr mt'>
        <view class="title">汽车资料</view>
        <view class='data'>
            <view class='items'>
                <view class='hd'>品牌:</view>
                <view class='bd'>{{village}}</view>
            </view>
            <view class='items'>
                <view class='hd'>车型:</view>
                <view class='bd'>{{acreage}}</view>
            </view>
        </view>
    </view>
    <view class='ocr mt'>
        <view class="title">申请金额</view>
        <view class='data'>
            <view class='items'>
                <view class='hd'>金额:</view>
                <view class='bd'>{{money}}</view>
            </view>
            <view class='items'>
                <view class='hd'>借多久:</view>
                <view class='bd'>{{howLong}}</view>
            </view>
        </view>
    </view>
</view>

