<view class="banner"></view>
<view class='ocr'>
    <view class="title">OCR身份证识别</view>
    <view class='ocrMain'>
        <view class='items'>
            <view class='hd'>姓名:</view>
            <view class='bd'>{{ocrName}}</view>
        </view>
        <view class='items'>
            <view class='hd'>性别:</view>
            <view class='bd'>{{ocrSex}}</view>
        </view>
        <view class='items'>
            <view class='hd'>民族:</view>
            <view class='bd'>{{ocrNation}}</view>
        </view>
        <view class='items'>
            <view class='hd'>出生:</view>
            <view class='bd'>{{ocrBirth}}</view>
        </view>
        <view class='items'>
            <view class='hd'>地址:</view>
            <view class='bd'>{{ocrAddress}}</view>
        </view>
        <view class='items'>
            <view class='hd'>身份证号:</view>
            <view class='bd'>{{ocrIdNumber}}</view>
        </view>
        <view class='items'>
            <view class='hd'>签发机关:</view>
            <view class='bd'>{{ocrOffice}}</view>
        </view>
        <view class='items'>
            <view class='hd'>有效期限:</view>
            <view class='bd'>{{ocrValidityPeriod}}</view>
        </view>
    </view>
</view>

