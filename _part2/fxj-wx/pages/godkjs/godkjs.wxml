<view class='title'>额度快测输入</view>
<view class="data">
    <view class='items'>
         <view class='hd'>估值/万:</view>
         <view class='bd'>
            <input type='text' placeholder-class="placeholder" value='{{lsnum}}' bindinput='lsnumInput' placeholder='请输入估值/万'></input>
        </view>
    </view>
       <view class='items'>
         <view class='hd'>贷款成数:</view>
         <view class='bd'>
            <input type='text' placeholder-class="placeholder" value='{{lsnum2}}' bindinput='lsnum2Input' placeholder='请输入贷款成数'></input>
        </view>
    </view>
      <!-- <view class='items'>
         <view class='hd'>申请金额:</view>
         <view class='bd'>
            <input type='text' placeholder-class="placeholder" value='{{money}}' bindinput='moneyInput' placeholder='请输入申请金额'></input>
        </view>
    </view> -->
    <view class='items'>
         <view class='hd'>月综合利息:</view>
         <view class='bd'>
            <input type='text' placeholder-class="placeholder" value='{{frameno}}' bindinput='framenoInput' placeholder='请输入月综合利息'></input>
        </view>
    </view>
       <view class='items'>
         <view class='hd'>年息:</view>
         <view class='bd'>
            <input type='text' placeholder-class="placeholder" value='{{frameno1}}' bindinput='frameno1Input' placeholder='请输入年息'></input>
        </view>
    </view>
    <view class='items'>
               <view class='hd'>申请期数/月:</view>
               <view class='bd'>
            <input type='text' placeholder-class="placeholder" value='{{engineno}}' bindinput='enginenoInput' placeholder='请输入申请期数/月'></input>
        </view>
    </view>

      <view class='items'>
               <view class='hd'>保险费:</view>
               <view class='bd'>
            <input type='text' placeholder-class="placeholder" value='{{insurance}}' bindinput='insuranceInput' placeholder='请输入保险费'></input>
        </view>
    </view>
 <view class='items'>
               <view class='hd'>返佣比例:</view>
               <view class='bd'>
            <input type='text' placeholder-class="placeholder" value='{{fanyong}}' bindinput='fanyongInput' placeholder='请输入返佣比例'></input>
        </view>
    </view>
  <view class='items'>
               <view class='hd'>GPS费用:</view>
               <view class='bd'>
            <input type='text' placeholder-class="placeholder" value='{{gps}}' bindinput='gpsInput' placeholder='请输入GPS费用'></input>
        </view>
    </view>
      <!-- <view class='items'>
         <view class='bd'>银行方费率:</view>
