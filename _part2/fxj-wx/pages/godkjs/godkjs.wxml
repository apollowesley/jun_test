            <input type='text' placeholder-class="placeholder" value='{{engineno1}}' bindinput='engineno1Input' placeholder='请输入银行方费率'></input>
        </view>
    </view> -->
    <view class="btn" bindtap='sbumitBtn'>一键快测</view>
</view>

<view class='wrap'>
    <view class='title'>额度快测结果</view>
    <view class='resultBox'>
        <view class='list'>
            <view class='l'>可申请金额:</view>
            <view class='r'>{{result1}}</view>
        </view>
        <view class='list'>
            <view class='l'>总贷款额:</view>
            <view class='r'>{{result2}}</view>
        </view>
         <view class='list'>
            <view class='l'>本息总和:</view>
            <view class='r'>{{result3}}</view>
        </view>
          <view class='list'>
            <view class='l'>月供:</view>
            <view class='r'>{{result4}}</view>
        </view>
          <view class='list'>
            <view class='l'>佣金:</view>
            <view class='r'>{{result5}}</view>
        </view>
          <view class='list'>
            <view class='l'>合同贷款额:</view>
            <view class='r'>{{result6}}</view>
        </view>
