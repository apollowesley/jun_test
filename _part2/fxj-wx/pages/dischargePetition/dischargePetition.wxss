page {
    background-color: #FFFFFF;
}
.title {
    width: 100%;
    height:100rpx;
    line-height: 100rpx;
    background-color: #FFFFFF;
    position: fixed;
    z-index: 1;
    top:0;
    left:0;
    padding: 0 30rpx;
    overflow: hidden;
    box-sizing: border-box;
    border-bottom: 1px solid #eeeeee;
}

.title .hd {
    float: right;
}

.title .bd {
    white-space: nowrap;
    overflow: hidden;
    display: block;
    text-overflow: ellipsis;
}

.list {
    width: 100%;
    overflow: hidden;
    padding-top:100rpx;
}

.list .items {
    width: 100%;
    border-bottom: 1px solid #eeeeee;
    padding: 20rpx 30rpx;
    box-sizing: border-box;
}

.list .items .t {
    overflow: hidden;
    font-size: 32rpx;
    line-height:50rpx;
    height:50rpx;
}

.list .items .t .hd {
    float: right;
    color: #3c8cf0;
}

.list .items .t .bd {
    white-space: nowrap;
    overflow: hidden;
    display: block;
    text-overflow: ellipsis;
}

.list .items .b {
    overflow: hidden;
    color: #999999;
    line-height:50rpx;
    height:50rpx;
}

.list .items .b .hd {
    float: right;
}

.list .items .b .bd {
    white-space: nowrap;
    overflow: hidden;
    display: block;
    text-overflow: ellipsis;
}

.btnBar {
    text-align: center;
    margin-top: 80rpx;
}

.btnBar .btn {
    width: 292rpx;
    height: 80rpx;
    line-height: 80rpx;
    color: #fff;
    background-color: #df6330;
    display: inline-block;
    margin:0 20rpx;
    border-radius: 12rpx;
}

.btnBar .btn::before {
    font-family: iconfont;
    content: "\e722";
    font-size: 44rpx;
    display: inline-block;
    vertical-align: middle;
    margin-right: 10rpx;
}

.btnBar .btn.blue {
    background-color: #7aa6d1;
}
.btnBar .btn.blue::before {
    content: "\e602";
}