<view class='ocr mt'>
    <view class="title">方案选择</view>
    <view class='data'>
        <view class='priceItems'>
            <view class='list' bindtap='chooesAmount' data-amount="50000">5万</view>
            <view class='list' bindtap='chooesAmount' data-amount="100000">10万</view>
            <view class='list' bindtap='chooesAmount' data-amount="200000">20万</view>
            <view class='list' bindtap='chooesAmount' data-amount="300000">30万</view>
        </view>

        <view class='items'>
            <view class='bd'>
                <input type='number' placeholder-class="placeholder" placeholder="请输入金额" class='h88' value='{{orderAmount}}' bindinput='orderAmountInput'></input>
            </view>
        </view>

        <view class='items'>
            <view class='hd'>借多久(月):</view>
            <view class='bd relative'>
                <picker value="{{pickerIndex}}" range="{{array}}" bindchange="bindRelationship">
                    <input value='{{array[pickerIndex]}}' placeholder='请选择期限' disabled='true' class='pr' />
                </picker>
            </view>
        </view>
        <view class='items'>
            <view class='hd'>利率:</view>
            <view class='bd'>
                <input type='text' placeholder-class="placeholder" cursor-spacing="120rpx" placeholder="请填写利率" value='{{orderBy14}}' bindinput='orderBy14Input'></input>
            </view>
        </view>
        <view class='items'>
            <view class='hd'>GPS费:</view>
            <view class='bd'>
                <input type='text' placeholder-class="placeholder" cursor-spacing="120rpx" placeholder="请填写GPS费" value='{{orderBy15}}' bindinput='orderBy15Input'></input>
            </view>
        </view>
        <view class='items'>
            <view class='hd'>服务费:</view>
            <view class='bd'>
                <input type='text' placeholder-class="placeholder" cursor-spacing="120rpx" placeholder="请填写服务费" value='{{orderBy16}}' bindinput='orderBy16Input'></input>
            </view>
        </view>
        <view class='items'>
            <view class='hd'>其他:</view>
            <view class='bd'>
                <input type='text' placeholder-class="placeholder" cursor-spacing="120rpx" placeholder="请填写其他费" value='{{orderBy17}}' bindinput='orderBy17Input'></input>
            </view>
        </view>
    </view>
</view>

<view class='ocr mt'>
    <view class='title'>添加资料</view>
    <view class='data'>
        <view class='items wwq'>
            <view class='items-btn' data-tit="增加共借人" data-orderNo="{{orderNo}}-01" bindtap="goGuarantee">增加共借人</view>
