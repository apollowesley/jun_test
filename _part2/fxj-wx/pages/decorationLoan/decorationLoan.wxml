            <label class="radio">
                <radio value="是" color='#7aa6d1' /> 是
            </label>
            <label class="radio">
                <radio value="否" color='#7aa6d1' /> 否
            </label>
        </radio-group>
    </view>
</view>

  <view class='items'>
            <view class='hd'>学历:</view>
            <view class='bd'>
                <input type='text' placeholder-class="placeholder" cursor-spacing="120rpx" placeholder="请填写学历" value='{{orderBy5}}' bindinput='orderBy5Input'></input>
            </view>
        </view>

          <view class='items'>
            <view class='hd'>工作单位:</view>
            <view class='bd'>
