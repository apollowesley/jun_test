page {
    padding-bottom: 40rpx;
}

.bannerspmq {
    width: 100%;
    height: 50rpx;
    background-size: cover;
}
.Xycx {
    box-sizing: border-box;
    margin: 0 34rpx 30rpx;
    width: 260rpx;
    height: 80rpx;
    line-height: 76rpx;
    border-radius: 40rpx;
    border: 2rpx solid #6a5038;
    text-align: center;
    float: left;
    color: #6a5038;
}
.banner {
    width: 100%;
    height: 400rpx;
    background: url(https://www.zhaodui.com.cn/pic/logo.jpg) no-repeat;
    background-size: cover;
}
.ocr {
    margin: -60rpx 42rpx 0;
    background-color: #fff;
    border-radius: 20rpx;
    overflow: hidden;
    padding: 20rpx 0;
    position: relative;
}

.ocr.mt {
    margin-top: 26rpx;
}

.ocr .title {
    height: 70rpx;
    line-height: 70rpx;
    margin: 0 34rpx;
    padding-left: 16rpx;
    color: #6a5038;
    position: relative;
    font-size: 32rpx;
    font-weight: bold;
}

.ocr .title::before {
    content: '';
    width: 6rpx;
    height: 30rpx;
    background-color: #2a2a2a;
    position: absolute;
    left: 0;
    top: 20rpx;
}

.ocr .title .certificate {
    float: right;
    color: #3c8cf0;
    font-size: 28rpx;
    font-weight: normal;
}

.ocr .title .certificate::after {
    font-family: iconfont;
    content: "\e62a";
    font-size: 36rpx;
    vertical-align: middle;
    display: inline-block;
    margin-left: 4rpx;
    font-weight: normal;
}

.ocrMain {
    padding: 0 50rpx;
}

.ocrMain .items {
    width: 100%;
    display: -webkit-box;
    line-height: 52rpx;
    padding: 12rpx 0;
}

.ocrMain .items .hd {
    padding-right: 20rpx;
}

.ocrMain .items .bd {
    -webkit-box-flex: 1;
    box-flex: 1;
    color: #999;
}

.ocrMain .items .bd input {
    width: 100%;
    height: 52rpx;
    min-height: 52rpx;
    color: #2a2a2a;
    font-size: 28rpx;
}

.ocrUpload {
    padding: 30rpx 0 0 7rpx;
    overflow: hidden;
}

.ocrUploadBtn {
    box-sizing: border-box;
    margin: 0 34rpx 30rpx;
    width: 260rpx;
    height: 80rpx;
    line-height: 76rpx;
    border-radius: 40rpx;
    border: 2rpx solid #6a5038;
    padding-left: 30rpx;
    float: left;
    color: #6a5038;
}

.ocrUploadBtn::before {
    font-family: iconfont;
    content: "\e60f";
    font-size: 44rpx;
    vertical-align: middle;
    display: inline-block;
    margin-right: 4rpx;
}

.data {
    padding: 0 50rpx;
}

.data .items {
    width: 100%;
    display: -webkit-box;
    line-height: 52rpx;
    padding: 12rpx 0;
}

.data .priceItems {
    margin:0 -50rpx 0 -30rpx;
    overflow: hidden;
}

.data .priceItems .list{
    width:132rpx;
    height:72rpx;
    text-align:center;
    line-height:72rpx;
    float:left;
    margin:30rpx 0 0 20rpx;
    background-color:#f6f6f6;
    border-radius:6px;

}

.data .items .hd {
    width: 172rpx;
}

.data .items .bd input {
    width: 100%;
    height: 52rpx;
    min-height: 52rpx;
    font-size: 28rpx;
    color: #2a2a2a;
}

.data .items .bd input.h88 {
    height: 88rpx;
    border-bottom: 1px solid #eeeeee;
}

.data .items .bd {
    -webkit-box-flex: 1;
    box-flex: 1;
    color: #7a7a7a;
}

.data .items .bd.relative {
    position: relative;
}
.data .items .bd.relative::after {
    position: absolute;
    top:0;
    right:0;
    width: 70rpx;
    text-align: center;
    height: 60rpx;
    line-height: 60rpx;
    font-family: iconfont;
    content: "\e7cc";
    font-size: 44rpx;
    color: #999999;
    z-index: 0;
}

.data .items .bd input.pr {
    padding-right: 80rpx;
    position: relative;
    z-index: 2;
    box-sizing: border-box;
}


.imgMain {
    padding:10rpx 0 0 50rpx;
}
.imgMain .items {
    width:100%;
    color:#6a5038;
    margin-top: 20rpx;
}
.imgMain .items::after {
    content: "\200B";
    display: block;
    clear: both;
    height: 0;
}

.imgMain .hd {
    width:130rpx;
    height:130rpx;
    background-color: #f7f7f7;
    border-radius: 12rpx;
    float: left;
}

.imgMain .con {
    width:130rpx;
    height:130rpx;
    display: table-cell;
    vertical-align: middle;
    text-align: center;
}

.imgMain .hd .ico {width: 60rpx;height:60rpx;line-height:60rpx;margin: auto;}
.imgMain .hd .ico::before {
    font-family: iconfont;
    content: "\e60f";
    font-size:52rpx;
    
}
.imgMain .hd .text {
    font-size: 20rpx;
}
.imgMain .bd {
    margin-left: 130rpx;
}

.uploadimg {
    width:100%;
}

.uploadimg::after {
    content: "\200B";
    display: block;
    clear: both;
    height: 0;
}

.uploadimg .items {
    width: 130rpx;
    height: 130rpx;
    box-sizing: border-box;
    border: 2rpx solid #eee;
    float: left;
    margin:0 0 30rpx 26rpx;
    text-align: center;
    position: relative;
    border-radius: 12rpx;
}

.uploadimg .items::before {
    font-family: iconfont;
    font-size: 60rpx;
    content: '\e633';
    color: #ccc;
    line-height: 126rpx;
}

.uploadimg .items .pic {
    position: absolute;
    left: 0;
    top: 0;
    width:126rpx;
    height:126rpx;
    background-position: center;
    background-repeat: no-repeat;
    background-size: cover;
}


.uploadimg .items .pic image {
    width:126rpx;
    height:126rpx;
    display: block;
    border-radius: 12rpx;
}

.uploadimg .items .delete {
    position: absolute;
    top: -20rpx;
    right: -16rpx;
    width: 52rpx;
    height: 52rpx;
    background: #999;
    border-radius: 50%;
    border: 2rpx solid #fff;
    display: block;
    color: #fff;
    z-index: 10;
    text-align: center;
    line-height:48rpx;
}

.uploadimg .items .delete::after {
    content: '\e62f';
    font-size: 44rpx;
    font-family: iconfont;
    color: #fff;
    line-height:48rpx;
}

