<view class="banner"></view>

<view class='ocr mt1'>
    <view class="title">放款明细</view>
    <view class='data'>
    <view class='items'>
            <view class='hd'>放款金额:</view>
            <view class='bd'>
                <input type='text' readonly='readonly' placeholder-class="placeholder" cursor-spacing="120rpx" placeholder="请填写放款金额" value='{{orderAmount}}' ></input>
            </view>
        </view>

          <view class='items'>
            <view class='hd'>放款笔数:</view>
            <view class='bd'>
                <input type='text' placeholder-class="placeholder" cursor-spacing="120rpx" placeholder="请填写放款笔数" value='{{orderBy41}}' bindinput='orderBy41Input'></input>
            </view>
        </view>

          <view class='items'>
            <view class='hd'>平均放款金额:</view>
            <view class='bd'>
                <input type='text' placeholder-class="placeholder" cursor-spacing="120rpx" placeholder="请填写平均放款金额" value='{{orderBy42}}' bindinput='orderBy42Input'></input>
            </view>
        </view>
    </view>
</view>

<view class='ocr mt'>
    <view class="title">汇款明细</view>
    <view class='data'>
    <view class='items'>
            <view class='hd'>GPS费用:</view>
            <view class='bd'>
