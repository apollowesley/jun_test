                <input type='text' placeholder-class="placeholder" cursor-spacing="120rpx" placeholder="请填写保证金" value='{{orderBy43}}' bindinput='orderBy43Input'></input>
            </view>
        </view>

          <view class='items'>
            <view class='hd'>中介费:</view>
            <view class='bd'>
                <input type='text' placeholder-class="placeholder" cursor-spacing="120rpx" placeholder="请填写中介费" value='{{orderBy44}}' bindinput='orderBy44Input'></input>
            </view>
        </view>

         <view class='items'>
            <view class='hd'>保险费:</view>
            <view class='bd'>
                <input type='text' placeholder-class="placeholder" cursor-spacing="120rpx" placeholder="请填写保险费" value='{{orderBy45}}' bindinput='orderBy45Input'></input>
            </view>
        </view>

         <view class='items'>
            <view class='hd'>押金:</view>
            <view class='bd'>
                <input type='text' placeholder-class="placeholder" cursor-spacing="120rpx" placeholder="请填写押金" value='{{orderBy46}}' bindinput='orderBy46Input'></input>
