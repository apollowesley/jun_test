            </view>
        </view>

         <view class='items'>
            <view class='hd'>回款总计:</view>
            <view class='bd'>
                <input type='text' placeholder-class="placeholder" cursor-spacing="120rpx" placeholder="请填写回款总计" value='{{orderBy48}}' bindinput='orderBy48Input'></input>
            </view>
        </view>
    </view>
</view>

<view class='ocr mt'>
    <view class="title">内返明细</view>
    <view class='data'>
    <view class='items'>
            <view class='hd'>中间费:</view>
            <view class='bd'>
                <input type='text'  placeholder-class="placeholder" cursor-spacing="120rpx" placeholder="请填写中间费" value='{{orderBy49}}' bindinput='orderBy49Input' ></input>
            </view>
        </view>

          <view class='items'>
            <view class='hd'>内返金额:</view>
            <view class='bd'>
                <input type='text' placeholder-class="placeholder" cursor-spacing="120rpx" placeholder="请填写内返金额" value='{{orderBy50}}' bindinput='orderBy50Input'></input>
            </view>
        </view>
    </view>
</view>

<view class='ocr mt'>
    <view class="title">上传附件</view>
    <view class='imgMain'>
        <view class='items' wx:for="{{fxjProAttList}}" wx:if ="{{item.attType ==130}}" wx:key="id">
            <view class='hd'>
                <view class='con'>
                    <view class='ico'></view>
                    <view class='text'>{{item.attName}}</view>
                </view>
            </view>
            <view class='bd'>
                <view class='uploadimg'>
                    <view class='items' wx:for='{{item.fxjOrderAttaList}}' wx:key='id' wx:for-index='idx'>
                        <view class='delete' data-img-idx="{{idx}}" data-index="{{index}}" bindtap='deletePic'></view>
                        <view class='pic' style='background-image:url({{item.attaUrl}})' data-picId="{{item.picId}}"></view>
                    </view>
                    <view class='items'>
                        <view class='pic' bindtap='getEnclosurePic' data-attType="{{item.attType}}" data-index="{{index}}"></view>
                    </view>
                </view>
            </view>
        </view>
    </view>
</view>

<view class='btnBar'>
    <!-- <view class='btn' data-proNo="{{proNo}}" bindtap='saveBtn'>保存</view> -->
    <view class='btn blue' data-proNo="{{proNo}}" bindtap='submitBtn'>提交</view>
</view>