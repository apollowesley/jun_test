.myDraft {
  padding:0 34rpx;
  overflow: hidden;
  margin-top: 85rpx;
}
.myDraft .items {
    background-color: #FFFFFF;
    margin-top:24rpx;
    color: #7a7a7a;
    border-radius: 12rpx;
    width:100%;
    float: left;
}

.myDraft .hd {
    height: 76rpx;
    line-height: 74rpx;
    border-bottom: 2rpx #eee dotted;
    padding:0 20rpx 0 100rpx;
    font-size: 32rpx;
    position: relative;
    display: flex;
}

.myDraft .hd .r {
  float: right
}

.myDraft .hd .l {
    white-space:nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    word-break: break-all;
    word-wrap: break-word;
}

.myDraft .hd::before {
    width: 60rpx;
    height: 74rpx;
    line-height: 74rpx;
    font-family: iconfont;
    content: "\e620";
    position: absolute;
    left: 26rpx;
    top: 0;
    font-size: 48rpx;
    color: #7aa6d1;
}

.myDraft .bd {
    padding: 20rpx 0 30rpx 34rpx;
}

.myDraft .t {
    display: -webkit-box;
    height: 50rpx;
    line-height: 50rpx;
    overflow: hidden;
    padding-right: 34rpx
}

.myDraft .t .l, .myDraft .t .c, .myDraft .t .r {
    padding-right: 20rpx;
    white-space:nowrap;
    overflow: hidden;
    display: block;
    text-overflow: ellipsis;
    word-break: break-all;
    word-wrap: break-word;
}

.myDraft .t .l {
    width: 300rpx;
}

.myDraft .t .c {
    width: 200rpx;
}
.myDraft .t .c100 {
    width: 100%;
}
.myDraft .t .r {
    width: 240rpx;
    text-align: right;
    padding-right: 0;
}

.myDraft .middle {
    height: 50rpx;
    line-height: 50rpx;
    width: 100%;
    overflow: hidden;
}

.myDraft .middle .l,.myDraft .middle .c {
    width:50%;
    float: left;
    white-space:nowrap;
    overflow: hidden;
    display: block;
    text-overflow: ellipsis;
    word-break: break-all;
    word-wrap: break-word;
    box-sizing: border-box;
    padding-right:34rpx;
}
.myDraft .middle .c {
    text-align: right;
}

.myDraft .b {
    padding-top:20rpx;
    overflow: hidden;
}
.myDraft .b .btn {
    overflow: hidden;
    width:45%;
    text-align: center;
    border:2rpx solid #7aa6d1;
    height:64rpx;
    line-height:60rpx;
    box-sizing: border-box;
    font-size: 24rpx;
    border-radius:12rpx;
    float:left;
    margin-right: 30rpx;
}
.myDraft .b .btn.bgBlur {
    background-color: #7aa6d1;
    color: #FFFFFF;
}