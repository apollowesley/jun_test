        </view>
        <view class='items'>
            <view class='hd'>GPS联系人电话:</view>
            <view class='bd'>
                <input type='text' placeholder-class="placeholder" value='{{gpsInsMobile}}' bindinput='gpsInsMobileInput'></input>
            </view>
        </view>
        <view class='items'>
            <view class='hd'>开户行:</view>
            <view class='bd'>
                <input type='text' placeholder-class="placeholder" value='{{fangkBank}}' bindinput='fangkBankInput'></input>
            </view>
        </view>
        <view class='items'>
            <view class='hd'>户名:</view>
            <view class='bd'>
                <input type='text' placeholder-class="placeholder" value='{{fangkName}}' bindinput='fangkNameInput'></input>
            </view>
        </view>
        <view class='items'>
            <view class='hd'>卡号:</view>
            <view class='bd'>
                <input type='text' placeholder-class="placeholder" value='{{fangkId}}' bindinput='fangkIdInput'></input>
            </view>
        </view>
    </view>
