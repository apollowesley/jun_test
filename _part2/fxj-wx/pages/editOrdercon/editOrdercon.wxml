            </view>
        </view>
        <view class='items'>
            <view class='hd'>GPS安装时间:</view>
            <view class='bd'>
              <picker mode="multiSelector" value="{{gpsInsData}}" bindchange="changeDateTime1" bindcolumnchange="changeDateTimeColumn1" range="{{gpsInsDataArray1}}">
    <view class="tui-picker-detail">
        {{gpsInsDataArray1[0][gpsInsData[0]]}}-{{gpsInsDataArray1[1][gpsInsData[1]]}}-{{gpsInsDataArray1[2][gpsInsData[2]]}} {{gpsInsDataArray1[3][gpsInsData[3]]}}:{{gpsInsDataArray1[4][gpsInsData[4]]}}
