<view>

  <view class='express-container'>

    <view class='express-item' wx:for="{{loglist}}" wx:key="*this">

      <view class='express-left-date'>
        <view>{{item.logDate}}</view>
      </view>

      <!-- 左边子容器 -->
      <view class='express-middle-axis'>
        <!-- 正在进行的时间轴上半个时间线 -->
        <view class='online-top-closing'></view>
        <!-- 正在进行的时间轴点 -->
        <view class='dot-closing'></view>
        <!-- 正在进行的时间轴下半个时间线 -->
        <view class='online-bottom'></view>
        <!-- 显示尾部的圆点 -->
        <view class='dot-closing' wx:if="{{index==list.length - 1}}"></view>
      </view>

      <!-- 右边子容器 -->
      <view class='express-right'>
        <view class="bbb"><text>{{item.logUser}}</text></view>
        <view class='express-status-address'>{{item.bpmStatus}}{{item.logContent}}</view>
      </view>

    </view>

  </view>

</view>
