<view class="centerTop">
    <view class='hd' style='background-image:url({{rawData.avatarUrl}})'></view>
</view>
<view class='lendingVolume'>
    <view class='title'>历史总金额</view>
    <view class='text'>{{list.fxjx1}}</view>
</view>

<view class='report'>
    <view class='items'>
        <view class='title'>今日报表</view>
        <view class='p'>已完成<text>{{list.fxjx2}}</text></view>
        <view class='p'>未完成<text>{{list.fxjx3}}</text></view>
        <view class='p'>撤销<text>{{list.fxjx4}}</text></view>
        <view class='p'>新增<text>{{list.fxjx5}}</text></view>
