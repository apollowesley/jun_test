.data {
    background-color: #FFFFFF;
}

.data .items {
    width: 100%;
    display: -webkit-box;
    line-height: 52rpx;
    padding:20rpx;
    border-bottom:1px solid #EEEEEE;
    box-sizing: border-box;
}
.data .items:last-child {
    border-bottom: none;
}
.data .items .hd {
    width: 160rpx;
}

.data .items .bd input {
    width: 100%;
    height: 52rpx;
    min-height: 52rpx;
    font-size: 28rpx;
    color: #2a2a2a;
}

.data .items .bd {
    -webkit-box-flex: 1;
    box-flex: 1;
    color: #7a7a7a;
}

.btn {
  width: 600rpx;
    height:88rpx;
    line-height: 88rpx;
    text-align: center;
    color: #FFFFFF;
    font-size: 32rpx;
    background-color: #7aa6d1;
    margin:40rpx auto 0;
    border-radius: 12rpx;
}